{
  "expo": {
    "name": "yalla",
    "slug": "yalla",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "fonts": {
      "Outfit-Black": "./src/assets/fonts/Outfit-Black.ttf",
      "Outfit-Bold": "./src/assets/fonts/Outfit-Bold.ttf",
      "Outfit-Light": "./src/assets/fonts/Outfit-Light.ttf",
      "Outfit-Medium": "./src/assets/fonts/Outfit-Medium.ttf",
      "Outfit-Regular": "./src/assets/fonts/Outfit-Regular.ttf",
      "Outfit-SemiBold": "./src/assets/fonts/Outfit-SemiBold.ttf"
    },
    "ios": {
      "supportsTablet": true,
      "homeIndicatorAutoHidden": false,
      "homeIndicatorBackgroundColor": "#ffffff",
      "bundleIdentifier": "com.tahahasan7.yalla"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.tahahasan7.yalla"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
